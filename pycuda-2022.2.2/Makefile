.PHONY	: all install clean tags dist userdoc devdoc
# DO NOT EDIT THIS FILE -- it was generated by configure.py
# configure.py --cuda-root=/home/wxh/cuda

all: tags
	/opt/conda/bin/python setup.py build

dist:
	/opt/conda/bin/python setup.py sdist

install: tags
	/opt/conda/bin/python setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

tests:
	echo "running tests"
	find ./test -type f -name "*.py" -exec python {} \;
